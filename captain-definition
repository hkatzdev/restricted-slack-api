{
	"schemaVersion": 2,
	"dockerfileLines": [
    "FROM hayd/alpine-deno:1.0.1",
    "EXPOSE 8080",
    "WORKDIR /app",
    "USER deno",
    "COPY deps.ts .",
    "RUN deno cache deps.ts",
    "ADD . .",
    "RUN deno cache mod.ts",
    "CMD ["run", "--allow-env", "--allow-net=:8080", "mod.ts"]",
	]
}